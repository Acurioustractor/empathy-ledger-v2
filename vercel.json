{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  },
  "github": {
    "autoAlias": true,
    "enabled": true,
    "silent": false
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps --include=dev",
  "framework": "nextjs",
  "regions": ["syd1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, max-age=0" }
      ]
    }
  ]
}

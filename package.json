  "private": true,
  "scripts": {
    "dev": "next dev -p 3030",
    "dev:turbo": "next dev --turbopack -p 3030",
    "dev:clean": "./start-dev.sh",
    "dev:custom": "next dev -p 4000",
    "build": "next build",
    "start": "next start -p 3030",
